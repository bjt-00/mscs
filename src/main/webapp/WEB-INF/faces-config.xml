<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">

	<!-- Message Resource Boundle -->
	<application>
		<resource-bundle>
			<base-name>sid</base-name>
			<var>sid</var>
		</resource-bundle>
	</application>
	<application>
		<locale-config>
			<default-locale>ur</default-locale>
			<supported-locale>en</supported-locale>
			<supported-locale>ur</supported-locale>
		</locale-config>
	</application>

	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
		    <from-action>#{userBean.actionListener}</from-action>
			<from-outcome>userForm</from-outcome>
			<to-view-id>index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
		    <from-action>#{userBean.actionListener}</from-action>
			<from-outcome>usersList</from-outcome>
			<to-view-id>index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
